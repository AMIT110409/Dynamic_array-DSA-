#User function Template for python3

class Solution:
    def DivisibleByEight(self,s):
        #code here
        num = s[-3:]
        if len(s)>3:
            if int(num)%8==0:
                return 1
            else:
                return -1
        else:
            if int(s)%8==0:
                return 1
            else:
                return -1
            
        
